# MySqlReplManager properties
MySqlReplManager.Context.rootDbName=mysql
MySqlReplManager.Context.dbName=MASTER
MySqlReplManager.Context.tableName=_MASTERSTATUS
MySqlReplManager.Context.tableCreatePostfix=type = InnoDB
MySqlReplManager.Context.checkDelayMillis=5000
MySqlReplManager.Context.purgeDelayMillis=300000
MySqlReplManager.Context.canPurgeBinLogs=true
MySqlReplManager.Context.ConnectionPool.masterConnStr=jdbc:mysql://localhost:3306/mysql?user=admin
MySqlReplManager.Context.ConnectionPool.slaveConnStr=jdbc:mysql://localhost:3307/mysql?user=admin

# LinguineSNMPWatch properties
